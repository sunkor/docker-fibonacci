{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "frontend",
      "image": "sunkor/fibonacci-stack-frontend",
      "hostname": "frontend",
      "essential": false,
      "memory": 128
    },
    {
      "name": "server",
      "image": "sunkor/fibonacci-stack-server",
      "hostname": "server",
      "essential": false,
      "memory": 128
    },
    {
      "name": "worker",
      "image": "sunkor/fibonacci-stack-worker",
      "hostname": "worker",
      "essential": false,
      "memory": 128
    },
    {
      "name": "nginx",
      "image": "sunkor/fibonacci-stack-nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["frontend", "server"],
      "memory": 128
    }
  ]
}
